<p id="notice"><%= notice %></p>

<h1>Projects Users</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects_users.each do |projects_user| %>
      <tr>
        <td><%= projects_user.project_id %></td>
        <td><%= projects_user.user_id %></td>
        <td><%= link_to 'Show', projects_user %></td>
        <td><%= link_to 'Edit', edit_projects_user_path(projects_user) %></td>
        <td><%= link_to 'Destroy', projects_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projects User', new_projects_user_path %>
